{
    "presets": [
        "react",
        "es2015",
        "stage-0"
    ],
    "plugins": [
        "transform-decorators-legacy",
        [
            "module-resolver",
            {
                "root": [
                    "./"
                ],
                "alias": {
					"src": "./src"
                }
            }
        ],
        [
            "import",
            {
                "libraryName": "antd",
                "style": true
            }
        ]
    ]
}
